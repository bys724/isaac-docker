# Isaac Lab Docker 환경 변수 설정
# 이 파일을 .env로 복사하고 필요에 따라 수정하세요.

# Display 설정 (X11 GUI 지원)
DISPLAY=:0

# NVIDIA GPU 설정
# 특정 GPU만 사용하려면 ID를 지정 (예: 0,1)
NVIDIA_VISIBLE_DEVICES=all

# WandB 설정 (선택사항)
# WandB 계정이 있는 경우 API 키와 프로젝트 이름을 설정하세요.
# WANDB_API_KEY=your_wandb_api_key_here
# WANDB_PROJECT=isaac-lab-experiments

# Isaac Lab 설치 옵션
# 강제로 재설치하려면 1로 설정
FORCE_REINSTALL=0

# 로깅 레벨
# DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO